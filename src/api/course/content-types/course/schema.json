{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "CourseName": {
      "type": "string",
      "required": true
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "mappedBy": "courses"
    },
    "slug": {
      "type": "uid",
      "targetField": "CourseName",
      "required": true
    },
    "Price": {
      "type": "decimal",
      "required": true
    },
    "facilitators": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::facilitator.facilitator",
      "private": true,
      "inversedBy": "courses"
    },
    "Location": {
      "type": "customField",
      "options": [
        "In Person",
        "Online"
      ],
      "customField": "plugin::multi-select.multi-select",
      "required": true
    },
    "courseCode": {
      "type": "enumeration",
      "enum": [
        "Wellbeing-01",
        "Wellbeing-02",
        "Wellbeing-03",
        "Relationship-01",
        "Relationship-02",
        "Relationship-03",
        "Corporate-01",
        "Corporate-02",
        "Corporate-03",
        "Community-01",
        "Community-02",
        "Community-03"
      ],
      "required": true
    },
    "courses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::course.course"
    },
    "courseDuration": {
      "type": "component",
      "repeatable": false,
      "component": "course-components.duration",
      "required": true
    },
    "courseObjectives": {
      "type": "component",
      "repeatable": true,
      "component": "course-components.course-objective",
      "required": true
    }
  }
}
